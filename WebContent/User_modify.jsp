<%@page import="java.util.Map"%><%@page import="java.util.List"%><%@page import="com.right.dao.MenuDAO"%><%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><html><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8"><link href="bootstrap/css/bootstrap.min.css" rel="stylesheet"><script src="jquery/jquery-1.9.1.min.js"></script><script src="bootstrap/js/bootstrap.min.js"></script><title>权限管理</title></head><body><script type="text/javascript">	$("#status").blur(function() {	//		alert("判断是数字");				if($.isNumeric($("#status").val())){		}else{			alert("status不是数字");			$("#status").val("");		}	});	function submit(){		$("#menuForm").submit();	}</script><div>		<div class="modal-header">			<button type="button" class="close" data-dismiss="modal"				aria-label="Close">				<span aria-hidden="true">&times;</span>			</button>			<h4 class="modal-title">修改用户</h4>		</div>		<div class="modal-body">			<form role="form" id="menuForm" action="user!update.action" method="post">								<input type="hidden" name="user.id" value="${user.id}"/>								<div class="form-group">					<label for="role">角色</label> 					<select name="user.role.id" class="form-control" >							<c:if test="${ empty user.role}">								<option value="-1"></option>								</c:if>							<option value="${user.role.id}">${user.role.name}</option>							<c:forEach items="${roleList}" var="role">								<c:if test="${role.name != user.role.name }">									<option value="${role.id}">${role.name }</option>								</c:if>								</c:forEach>					</select>				</div>												<div class="form-group">					<label for="status">status</label> <input type="number" value="${user.status }"						id="status" name="user.status" class="form-control" placeholder="请输入status 注意该选项必须填数字，否则报错 "/>				</div>				<div class="form-group">					<label for="address">address</label> <input type="text" value="${user.address }"						id="address" name="user.address" class="form-control" placeholder="请输入address"/>				</div>				<div class="form-group">					<label for="email">email</label> <input type="text" value="${user.email }"						id="email" name="user.email" class="form-control" placeholder="请输入email"/>				</div>				<div class="form-group">					<label for="loginname">loginname</label> <input type="text" value="${user.loginname }"						id="loginname" name="user.loginname" class="form-control" placeholder="请输入loginname"/>				</div>				<div class="form-group">					<label for="mobilephone">mobilephone</label> <input type="text" value="${user.mobilephone }"						id="mobilephone" name="user.mobilephone" class="form-control" placeholder="mobilephone"/>				</div>				<div class="form-group">					<label for="name">name</label> <input type="text" value="${user.name }"						id="name" name="user.name" class="form-control" placeholder="请输入name"/>				</div>				<div class="form-group">					<label for="password">password</label> <input type="text" value="${user.password }"						id="password" name="user.password" class="form-control" placeholder="请输入password"/>				</div>														</form>		</div>		<div class="modal-footer">			<button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>			<button type="button" class="btn btn-primary" onclick="submit()">确定</button>		</div>	</div></body></html>